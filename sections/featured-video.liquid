{% comment %}theme-check-disable UnusedAssign{% endcomment %}
{%- assign sc_id = '#shopify-section-' | append: section.id -%}

{{ 'section-simple-media.css' | asset_url | stylesheet_tag }}
{% if section.settings.controls == true %}
	{{ 'component-video-controls.css' | asset_url | stylesheet_tag }}
{% endif %}

{% comment %} STYLES {% endcomment %}

<style type='text/css'>
	#shopify-section-{{ section.id }} {
	    padding-top: {{ section.settings.padding_top | times: 0.50 | round: 0 }}px;
	    padding-bottom: {{ section.settings.padding_bottom | times: 0.50 | round: 0 }}px;
	}

	.simple-{{ section.id }} {
	    padding-top: {{ section.settings.inner_padding_top | times: 0.50 | round: 0 }}px;
	    padding-bottom: {{ section.settings.inner_padding_bottom | times: 0.50 | round: 0 }}px;
	}

	@media screen and (min-width: 749px) {
	    #shopify-section-{{ section.id }} {
	        padding-top: {{ section.settings.padding_top }}px;
	        padding-bottom: {{ section.settings.padding_bottom }}px;
	    }
	    .simple-{{ section.id }} {
	        padding-top: {{ section.settings.inner_padding_top }}px;
	        padding-bottom: {{ section.settings.inner_padding_bottom }}px;
	    }
	}

	{%- if section.settings.enable_absolute != true -%}

	    @media screen and ( min-width: 750px ) {
	        .simple-{{ section.id }}.small > .simple-block.content { width: 70%; height: auto; }
	        .simple-{{ section.id }}.medium > .simple-block.content { width: 50%; height: auto; }
	        .simple-{{ section.id }}.large > .simple-block.content { width: 30%; height: auto; }
	        .simple-{{ section.id }}.full > .simple-block.content { position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%); }
	    }

	{%- else -%}

	        {% if section.settings.mobile_positioning == 'centered_overlay' or section.settings.mobile_positioning == 'centered_after' %} @media screen and ( min-width: 750px ) { {% endif %}
	            .simple-{{ section.id }} > .simple-block.content {
	                position: absolute!important;
	                {%- if section.settings.enable_distance_top == true -%}top: {{ section.settings.content_top }}%;{%- endif -%}
	                {%- if section.settings.enable_distance_bottom == true -%}bottom: {{ section.settings.content_bottom }}%;{%- endif -%}
	                {%- if section.settings.enable_distance_left == true -%}left: {{ section.settings.content_left }}%;{%- endif -%}
	                {%- if section.settings.enable_distance_right == true -%}right: {{ section.settings.content_right }}%;{%- endif -%}

	                {%- if section.settings.enable_distance_top != true and section.settings.enable_distance_bottom != true and section.settings.enable_distance_left != true and section.settings.enable_distance_right != true -%} top: 50%; left: 50%; transform: translate(-50%, -50%); {%- endif -%}
	            }
	        {% if section.settings.mobile_positioning == 'centered_overlay' or section.settings.mobile_positioning == 'centered_after' %} } {% endif %}

	        {% if section.settings.mobile_positioning == 'centered_overlay' %}
	            @media screen and ( max-width: 750px ) {
	                .simple-{{ section.id }} > .simple-block.content {
	                    position: absolute!important;
	                    top: 50%;
	                    left: 50%;
	                    transform: translate(-50%, -50%);
	                }
	            }
	        {% endif %}

	        {% if section.settings.mobile_positioning == 'centered_after' %}
	            @media screen and ( max-width: 750px ) {
	                .simple-{{ section.id }} > .simple-block.content {
	                    position: relative;
	                }
	            }
	        {% endif %}

	{%- endif -%}

	@media screen and ( min-width: 750px ) {

	    .simple-{{ section.id }}.small > .simple-media { width: 30%; }
	    .simple-{{ section.id }}.medium > .simple-media { width: 50%; }
	    .simple-{{ section.id }}.large > .simple-media { width: 70%; }
	    .simple-{{ section.id }}.full > .simple-media { width: 100%; }

	    {% if section.settings.content_fit == false %}
	    .simple-{{ section.id }} .simple-block-content {
	        max-width: {{ section.settings.content_max_width }}px;
	        min-width: {{ section.settings.content_min_width }}px;
	    }
	    {% else %}
	    .simple-{{ section.id }} .simple-block-content {
	        max-width: fit-content;
	        width: fit-content;
	        max-height: fit-content;
	        height: fit-content;
	    }
	    .simple-{{ section.id }} .simple-block-content h2 {
	        white-space: nowrap;
	    }
	    {% endif %}
	}

	.simple-{{ section.id }}.full > .simple-media {
	    width: 100%;
	}

	@media screen and ( min-width: 750px ) {

	    .simple-{{ section.id }} {
	        position: relative;
	        max-width: {{ section.settings.section_max_width }}%;
	    }
	}

	@media screen and ( max-width: 749px ) {

	    .simple-{{ section.id }} .simple-block.simple-media {
	        width: 100%;
	    }
	}

	{%- if section.settings.section_height == "550" or section.settings.section_height == "700" or section.settings.section_height == "950" -%}

	    @media screen and ( min-width: 750px ) {

	        .simple-{{ section.id }} {
	            height: {{ section.settings.section_height }}px;
	            display: flex;
	        }

	        .simple-block > video {
	            position: relative;
	            object-fit: cover;
	            height: -webkit-fill-available;
	            width: -webkit-fill-available;
	            max-width: unset;
	            left: 50%;
	            transform: translateX(-50%);
	        }
	    }

	{%- endif -%}

	{%- if section.settings.section_height == "full" -%}

	    @media screen and ( min-width: 750px ) {

	        .simple-{{ section.id }} {
	            height: 100vh;
	        }

	        .simple-{{ section.id }} .simple-block > video {
	            position: relative;
	            object-fit: cover;
	            height: -webkit-fill-available;
	            width: -webkit-fill-available;
	            height: 100%;
	        }
	    }

	{% endif %}

	{%- if section.settings.section_height == 'adapt' -%}

	    @media screen and (max-width: 749px) {
	        .simple-{{ section.id }} .simple-media::before {
	            {% comment %} padding-bottom: 56.25%; /* 16:9 */ {% endcomment %}
	            padding-bottom: 68%;
	            {% comment %} padding-bottom: {{ 1 | divided_by: section.settings.video_url.aspect_ratio | times: 100 }}%; {% endcomment %}
	            content: '';
	            display: block;
	        }
	    }

	    @media screen and (min-width: 750px) {
	        .simple-{{ section.id }} .simple-media::before {
	            padding-bottom: 56.25%; /* 16:9 */
	            {% comment %} padding-bottom: {{ 1 | divided_by: section.settings.video_url.aspect_ratio | times: 100 }}%; {% endcomment %}
	            content: '';
	            display: block;
	        }
	    }

	    .simple-{{ section.id }}::after {
	        opacity: {{ section.settings.video_overlay_opacity | divided_by: 100.0 }};
	    }

	    #mainVideo{{ section.id | remove: "template--" }} {
	        display: block;
	        max-width: 100%;
	        position: absolute;
	        top: 50%;
	        left: 50%;
	        height: auto;
	        width: 100%;
	        transform: translate(-50%, -50%);
	    }

	    @media screen and (max-width: 750px) {
	        #mainVideo{{ section.id | remove: "template--" }} {
	            display: block;
	            max-width: 100%;
	            position: absolute;
	            top: 50%;
	            left: 50%;
	            height: auto;
	            width: 100%;
	            min-width: 140%;
	            transform: translate(-50%, -50%);
	        }
	    }

	{%- endif -%}

	/* content styles */

	.simple-{{ section.id }} .simple-block.content {
	    text-align: {{ section.settings.text_position_mobile }};
	}

	@media screen and ( min-width: 750px ) {

	    .simple-{{ section.id }} .simple-block.content {
	        text-align: {{ section.settings.text_position }};
	    }
	}

	/* media */

	@media screen and ( min-width: 750px ) {
	    .simple-{{ section.id }} .simple-block.simple-media {
	        padding-left: {{ section.settings.media_margin_left }}px;
	        padding-right: {{ section.settings.media_margin_right }}px;
	    }
	}

	{%- if section.settings.enable_max_media_width -%}

	    .simple-{{ section.id }} .simple-media video {
	        max-width: {{ section.settings.max_media_width }}px;
	    }

	    .simple-{{ section.id }} .simple-media video {
	        width: {{ section.settings.media_width }}%;
	    }

	{%- endif -%}
</style>

{% comment %} CONTENT {% endcomment %}

<div
	class='simple simple-{{ section.id }} color-{{ section.settings.color_scheme }} flx{% if section.settings.media_position == 'reverse' %} d-rr{% endif %} m-vrt{% if section.settings.reverse_mobile != true %} m-cr{% endif %} {{ section.settings.media_size }}'
	style='background-color: {{ section.settings.section_bg_color }};'
>
	<div
		class='
			hosted-video simple-block simple-media d-flx d-ctr-xy
			{% if section.settings.enable_animation %} animated {% endif %}
			 delay-{{ section.settings.animation_delay }}ms
			{{ section.settings.animation_speed }}
		'
		data-tca='
			if:     {{ section.settings.animation_trigger_1 }},
			{% if section.settings.animation_trigger_1 == 'scroll' %}on: window,{% endif %}
			do: {% if section.settings.animation_type_out_1 == 'none' %}{{ section.settings.animation_type_1 }}{% elsif section.settings.animation_type_1 == 'none' %}{{ section.settings.animation_type_out_1 }}{% endif %},
			before: scrollReveal
			{% if section.settings.animation_once_1 != true %}, after: removeAnim {% endif %};
			if:     {{ section.settings.animation_trigger_2 }},
			{% if section.settings.animation_trigger_2 == 'scroll' %}on: window,{% endif %}
			do: {% if section.settings.animation_type_out_2 == 'none' %}{{ section.settings.animation_type_2 }}{% elsif section.settings.animation_type_2 == 'none' %}{{ section.settings.animation_type_out_2 }}{% endif %}
			{% if section.settings.animation_once_2 != true %}, after: removeAnim {% endif %};
		'
	>
		{%- if section.settings.video_url != empty -%}
			{% comment %}theme-check-disable ImgLazyLoading{% endcomment %}
			<video
				id='mainVideo{{ section.id | remove: "template--" }}'
				{% if section.settings.muted == true %}
					muted
				{% endif %}
				{% if section.settings.autoplay == true %}
					autoplay
				{% endif %}
				{% if section.settings.loop == true %}
					loop
				{% endif %}
				{% if section.settings.playsinline == true %}
					playsinline
				{% endif %}
				{% if section.settings.autobuffer == true %}
					autobuffer
				{% endif %}
				{% if section.settings.preload == true %}
					preload
				{% endif %}
				{% if section.settings.enable_lazyload %}
					loading='lazy'
				{% endif %}
				{% if section.settings.poster != blank %}
					poster='{{ section.settings.poster | image_url: width: 1500 }}'
				{% endif %}
			>
				<source src='{{ section.settings.video_url }}' type='video/mp4'>
			</video>
		{%- else -%}
			{{ 'image' | placeholder_svg_tag: 'placeholder-image' }}
			{% comment %}theme-check-enable ImgLazyLoading{% endcomment %}
		{%- endif -%}

		{% if section.settings.controls == true %}
			{% render 'video-controls', section_id: section.id, autoplay: section.settings.autoplay, muted: section.settings.muted, position: section.settings.controls_position %}
		{% endif %}
	</div>

	<div class='simple-block content flx d-ctr-xy'>
		<div class='simple-block-content {{ section.settings.content_padding_desktop }} {{ section.settings.content_padding_mobile }}'>
			{%- for block in section.blocks -%}
				{%- case block.type -%}
					{%- when 'heading' -%}
						<h2
							{{ block.shopify_attributes }}
							class='
								{{ block.settings.heading_size }} {{ block.settings.heading_style }} mt0 {{ block.settings.heading_margin_bottom }}
								{% if block.settings.enable_animation %} animated {% endif %}
								 delay-{{ block.settings.animation_delay }}ms
								{{ block.settings.animation_speed }}
							'
							data-tca='
								if:     {{ block.settings.animation_trigger_1 }},
								{% if block.settings.animation_trigger_1 == 'scroll' %}on: window,{% endif %}
								do: {% if block.settings.animation_type_out_1 == 'none' %}{{ block.settings.animation_type_1 }}{% elsif block.settings.animation_type_1 == 'none' %}{{ block.settings.animation_type_out_1 }}{% endif %},
								before: scrollReveal
								{% if block.settings.animation_once_1 != true %}, after: removeAnim {% endif %};
								if:     {{ block.settings.animation_trigger_2 }},
								{% if block.settings.animation_trigger_2 == 'scroll' %}on: window,{% endif %}
								do: {% if block.settings.animation_type_out_2 == 'none' %}{{ block.settings.animation_type_2 }}{% elsif block.settings.animation_type_2 == 'none' %}{{ block.settings.animation_type_out_2 }}{% endif %}
								{% if block.settings.animation_once_2 != true %}, after: removeAnim {% endif %};
							'
						>
							{{ block.settings.heading }}
						</h2>
					{%- when 'text' -%}
						<div
							{{ block.shopify_attributes }}
							class='
								rich-text__text rte {{ block.settings.paragraph_style }} {{ block.settings.paragraph_margin_bottom }}
								{% if block.settings.enable_animation %} animated {% endif %}
								 delay-{{ block.settings.animation_delay }}ms
								{{ block.settings.animation_speed }}
							'
							data-tca='
								if:     {{ block.settings.animation_trigger_1 }},
								{% if block.settings.animation_trigger_1 == 'scroll' %}on: window,{% endif %}
								do: {% if block.settings.animation_type_out_1 == 'none' %}{{ block.settings.animation_type_1 }}{% elsif block.settings.animation_type_1 == 'none' %}{{ block.settings.animation_type_out_1 }}{% endif %},
								before: scrollReveal
								{% if block.settings.animation_once_1 != true %}, after: removeAnim {% endif %};
								if:     {{ block.settings.animation_trigger_2 }},
								{% if block.settings.animation_trigger_2 == 'scroll' %}on: window,{% endif %}
								do: {% if block.settings.animation_type_out_2 == 'none' %}{{ block.settings.animation_type_2 }}{% elsif block.settings.animation_type_2 == 'none' %}{{ block.settings.animation_type_out_2 }}{% endif %}
								{% if block.settings.animation_once_2 != true %}, after: removeAnim {% endif %};
							'
						>
							{{ block.settings.text }}
						</div>
					{%- when 'buttons' -%}
						<div
							{{ block.shopify_attributes }}
							class='
								rich-text__buttons
								{% if block.settings.enable_animation %} animated {% endif %}
								 delay-{{ block.settings.animation_delay }}ms
								{{ block.settings.animation_speed }}
							'
							data-tca='
								if:     {{ block.settings.animation_trigger_1 }},
								{% if block.settings.animation_trigger_1 == 'scroll' %}on: window,{% endif %}
								do: {% if block.settings.animation_type_out_1 == 'none' %}{{ block.settings.animation_type_1 }}{% elsif block.settings.animation_type_1 == 'none' %}{{ block.settings.animation_type_out_1 }}{% endif %},
								before: scrollReveal
								{% if block.settings.animation_once_1 != true %}, after: removeAnim {% endif %};
								if:     {{ block.settings.animation_trigger_2 }},
								{% if block.settings.animation_trigger_2 == 'scroll' %}on: window,{% endif %}
								do: {% if block.settings.animation_type_out_2 == 'none' %}{{ block.settings.animation_type_2 }}{% elsif block.settings.animation_type_2 == 'none' %}{{ block.settings.animation_type_out_2 }}{% endif %}
								{% if block.settings.animation_once_2 != true %}, after: removeAnim {% endif %};
							'
						>
							{%- if block.settings.button_label_1 != blank -%}
								<a
									data-tca='if: click, do: zoomOut'
									{{ block.shopify_attributes }}
									{% if block.settings.button_link_1 == blank %}
										role='link' aria-disabled='true'
									{% else %}
										href='{{ block.settings.button_link_1 }}'
									{% endif %}
									class='button animated {% if block.settings.button_label_2 != blank %} mr20{% endif %}{% if block.settings.button_style_secondary_1 %} button--secondary{% else %} button--primary{% endif %}'
								>
									{{ block.settings.button_label_1 | escape }}
								</a>
							{%- endif -%}
							{%- if block.settings.button_label_2 != blank -%}
								<a
									data-tca='if: click, do: zoomOut'
									{% if block.settings.button_link_2 == blank %}
										role='link' aria-disabled='true'
									{% else %}
										href='{{ block.settings.button_link_2 }}'
									{% endif %}
									class='button animated{% if block.settings.button_style_secondary_2 %} button--secondary{% else %} button--primary{% endif %}'
								>
									{{- block.settings.button_label_2 | escape -}}
								</a>
							{%- endif -%}
						</div>
				{%- endcase -%}
			{%- endfor -%}
		</div>
	</div>
</div>

{% comment %} SCHEMA {% endcomment %}

{% schema %}
	{
	    "name": "Featured Video",
	    "class": "featured-simple",
	    "settings": [
	        {
	            "type": "header",
	            "content": "Section specific"
	        },
	        {
	            "type": "select",
	            "id": "color_scheme",
	            "options": [
	                {
	                    "value": "accent-1",
	                    "label": "t:sections.all.colors.accent_1.label"
	                },
	                {
	                    "value": "accent-2",
	                    "label": "t:sections.all.colors.accent_2.label"
	                },
	                {
	                    "value": "background-1",
	                    "label": "t:sections.all.colors.background_1.label"
	                },
	                {
	                    "value": "background-2",
	                    "label": "t:sections.all.colors.background_2.label"
	                },
	                {
	                    "value": "inverse",
	                    "label": "t:sections.all.colors.inverse.label"
	                },
	                {
	                    "value": "transparent",
	                    "label": "No background"
	                }
	            ],
	            "default": "background-1",
	            "label": "t:sections.all.colors.label",
	            "info": "t:sections.image-banner.settings.color_scheme.info"
	        },
	        {
	            "type": "color",
	            "id": "section_bg_color",
	            "label": "Section background color",
	            "default": "#ffffff"
	        },
	        {
	            "type": "header",
	            "content": "Section layout",
	            "info": "In this section you can edit section specific attributes. Feel free to play around and test different combinations!"
	        },
	        {
	            "type": "range",
	            "id": "section_max_width",
	            "min": 0,
	            "max": 100,
	            "step": 1,
	            "unit": "%",
	            "label": "Section width",
	            "default": 100
	        },
	        {
	            "type": "select",
	            "id": "section_height",
	            "options": [
	                {
	                    "value": "full",
	                    "label": "Full screen"
	                },
	                {
	                    "value": "adapt",
	                    "label": "Adapt"
	                },
	                {
	                    "value": "950",
	                    "label": "Large"
	                },
	                {
	                    "value": "700",
	                    "label": "Medium"
	                },
	                {
	                    "value": "550",
	                    "label": "Small"
	                }
	            ],
	            "default": "adapt",
	            "label": "Section height"
	        },
	        {
	            "type": "select",
	            "id": "media_position",
	            "options": [
	                {
	                    "value": "normal",
	                    "label": "Left"
	                },
	                {
	                    "value": "reverse",
	                    "label": "Right"
	                }
	            ],
	            "default": "normal",
	            "label": "Media position"
	        },
	        {
	            "type": "select",
	            "id": "media_size",
	            "options": [
	                {
	                    "value": "small",
	                    "label": "30%"
	                },
	                {
	                    "value": "medium",
	                    "label": "50%"
	                },
	                {
	                    "value": "large",
	                    "label": "70%"
	                },
	                {
	                    "value": "full",
	                    "label": "100%"
	                }
	            ],
	            "default": "large",
	            "label": "Media size"
	        },
	        {
	            "type": "header",
	            "content": "Section padding"
	        },
	        {
	            "type": "range",
	            "id": "padding_top",
	            "min": 0,
	            "max": 100,
	            "step": 4,
	            "unit": "px",
	            "label": "Top padding",
	            "default": 0
	        },
	        {
	            "type": "range",
	            "id": "padding_bottom",
	            "min": 0,
	            "max": 100,
	            "step": 4,
	            "unit": "px",
	            "label": "Bottom padding",
	            "default": 0
	        },
	        {
	            "type": "header",
	            "content": "Section inner padding"
	        },
	        {
	            "type": "range",
	            "id": "inner_padding_top",
	            "min": 0,
	            "max": 100,
	            "step": 4,
	            "unit": "px",
	            "label": "Top padding",
	            "default": 0
	        },
	        {
	            "type": "range",
	            "id": "inner_padding_bottom",
	            "min": 0,
	            "max": 100,
	            "step": 4,
	            "unit": "px",
	            "label": "Bottom padding",
	            "default": 0
	        },
	        {
	            "type": "header",
	            "content": "Video"
	        },
	        {
	            "type": "url",
	            "id": "video_url",
	            "label": "Video"
	        },
	        {
	            "type": "header",
	            "content": "Video options"
	        },
	        {
	            "type": "checkbox",
	            "id": "autoplay",
	            "default": true,
	            "label": "Autoplay"
	        },
	        {
	            "type": "checkbox",
	            "id": "loop",
	            "default": true,
	            "label": "Loop"
	        },
	        {
	            "type": "checkbox",
	            "id": "muted",
	            "default": true,
	            "label": "Muted"
	        },
	        {
	            "type": "checkbox",
	            "id": "playsinline",
	            "default": true,
	            "label": "Plays inline"
	        },
	        {
	            "type": "checkbox",
	            "id": "autobuffer",
	            "default": true,
	            "label": "Auto buffer"
	        },
	        {
	            "type": "checkbox",
	            "id": "preload",
	            "default": true,
	            "label": "Preload"
	        },
	        {
	            "type": "checkbox",
	            "id": "controls",
	            "default": false,
	            "label": "Controls"
	        },
	        {
	            "type": "select",
	            "id": "controls_position",
	            "options": [
	                {
	                    "value": "top-left",
	                    "label": "t:sections.image-banner.settings.desktop_content_position.options__1.label"
	                },
	                {
	                    "value": "top-center",
	                    "label": "t:sections.image-banner.settings.desktop_content_position.options__2.label"
	                },
	                {
	                    "value": "top-right",
	                    "label": "t:sections.image-banner.settings.desktop_content_position.options__3.label"
	                },
	                {
	                    "value": "middle-left",
	                    "label": "t:sections.image-banner.settings.desktop_content_position.options__4.label"
	                },
	                {
	                    "value": "middle-center",
	                    "label": "t:sections.image-banner.settings.desktop_content_position.options__5.label"
	                },
